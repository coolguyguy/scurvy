existed = getgenv()["_coolguyguy/do/particleoptimizer/exist"]
if existed == nil or false then
	local list = {
		["particleemitter"] = true;
		['sparkles'] = true;
		['beam'] = true;
		['trail'] = true;
		['fire'] = true;
		['smoke'] = true;
		['highlight'] = true;
		['blur'] = true;
		['depthoffield'] = true;
		['sunrays'] = true;
		['bloomeffect'] = true;
		['colorcorrectioneffect'] = true;
	}
	local function isinlist(p)
		local find = list[string.lower(p.ClassName)]
		print(find)
		if find then
			return true else return false
		end
	end
	do --do for existing instances
		for _, obj in ipairs(game:GetDescendants()) do
			if isinlist(obj) then
	
				pcall(function()
					obj.Enabled = false
				end)
			end
		end
	end
	game.DescendantAdded:Connect(function(obj)
		if isinlist(obj) then
	
			pcall(function()
				obj.Enabled = false
			end)
		end
	end)
	
	pcall(function()
		local _fpslabel = Instance.new("TextLabel")
		_fpslabel.Parent = game:GetService("CoreGui"):WaitForChild("RobloxGui") or game.Players.LocalPlayer.PlayerGui:WaitForChild("Freecam") or nil
		_fpslabel.BackgroundTransparency = 1
		_fpslabel.TextColor3 = Color3.new(1,1,1)
		_fpslabel.Size = UDim2.fromOffset(50,25)
		_fpslabel.TextScaled = true
		local delta = 0
		game["Run Service"].RenderStepped:Connect(function(dt)
			delta = dt
		end)
		game.StarterGui:SetCore("SendNotification",{
			Title = "particle_opt",
			Text = "v1.3",
			Duration = 5,
				Icon = game.Players:GetUserThumbnailAsync(
					game.Players.LocalPlayer.UserId,
					Enum.ThumbnailType.HeadShot,
					Enum.ThumbnailSize.Size352x352
				)
		})
		
			while task.wait(1) do
			_fpslabel.Text = "fps: "..math.floor(1/delta)
		end
	end)
	getgenv()["_coolguyguy/do/particleoptimizer/exist"] = true
else
			game.StarterGui:SetCore("SendNotification",{
			Title = "particle_opt",
			Text = "this script is already ran? please rejoin if u want to re-execute",
			Duration = 5,
				Icon = game.Players:GetUserThumbnailAsync(
					game.Players.LocalPlayer.UserId,
					Enum.ThumbnailType.HeadShot,
					Enum.ThumbnailSize.Size352x352
				)
		})
end
